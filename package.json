{
  "name": "@wizishop/ng-wizi-bulma",
  "version": "8.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run watch-scss\" \"ng build ng-wizi-bulma --watch \" \"ng serve\"",
    "build:lib": "ng build ng-wizi-bulma --prod && npm run block-scss",
    "build:demo": "ng build ng-wizi-bulma-demo --prod",
    "build": "rm -rf dist && npm run build:lib && npm run build:demo && cp README.md dist/@wizishop/ng-wizi-bulma",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run build && firebase deploy",
    "publish:npm": "cd dist/@wizishop/ng-wizi-bulma && npm publish --access public && cd ../../../",
    "publish": "npm run deploy && npm run publish:npm",
    "precommit": "pretty-quick --staged",
    "prepack": "ng build ng-wizi-bulma && npm run block-scss",
    "watch-scss": "nodemon --watch './projects/ng-wizi-bulma/src/**/*.scss' -e scss -d 1 -x \"npm run block-scss\" ",
    "block-scss": "node ./scripts/scss-bundle.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.0.0",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "8.0.0",
    "@angular/compiler": "8.0.0",
    "@angular/core": "8.0.0",
    "@angular/forms": "8.0.0",
    "@angular/platform-browser": "8.0.0",
    "@angular/platform-browser-dynamic": "8.0.0",
    "@angular/router": "8.0.0",
    "bulma": "^0.7.5",
    "bulma-extensions": "^6.2.7",
    "bundle-scss": "^1.3.4",
    "colors": "^1.3.3",
    "core-js": "^2.5.4",
    "rxjs": "^6.5.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular-devkit/build-ng-packagr": "~0.800.0",
    "@angular-devkit/schematics": "^8.0.1",
    "@angular/cli": "^8.0.1",
    "@angular/compiler-cli": "8.0.0",
    "@angular/language-service": "8.0.0",
    "@types/jasmine": "~2.8.11",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~10.12.9",
    "codelyzer": "^5.0.1",
    "concurrently": "^4.1.0",
    "husky": "^2.4.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~2.0.0",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.3.0",
    "nodemon": "^1.18.9",
    "prettier": "1.17.1",
    "pretty-quick": "^1.11.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.1",
    "tsickle": "0.35.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5"
  }
}
